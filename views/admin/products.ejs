            </nav>
        </aside>
    <main class="main-wrap">
        <div class="content-header">
            <h2 class="content-title">Product list</h2>
            
        </div>
        <div class="card mb-4">
            <header class="card-header">
                <div class="row gx-3">
                    <div class="col-lg-4 col-md-6 me-auto" id="messageHead">
                        <!-- <button class="add-button" href="/admin/addProducts"> Add Products</button> -->
                        <a href="/admin/addProducts" class="btn bg-success-light">Add Products</a>
                    </div>
                    <div>
                        <h3 id="mssgBox"></h3>
                    </div>
                </div>
            </header>
            <!-- card-header end// -->
            <div class="card-body " id="dynamicTable">
                
                <div class="table-responsive">
                    <table  class="table table-bordered table-hover table-striped">
                        <thead>
                            <tr>
                                <th>NO</th>
                                <th>Image</th>
                                <th>Name</th>
                                <th>Quantity</th>
                                <th>Price</th>
                                <th>Category</th>
                                <th>Offer</th>
                                <th>Edit</th>
                                <th>Update</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% for(let i=0 ;i < products.length ; i++ ) { %>
                                <tr>
                                    <td>
                                        <%= i+1 %>
                                    </td>
                                    <td>
                                        <% console.log(products[i].images[0])%>
                                        <img src="/multerimages/<%=products[i].images[0] %>"
                                            style="max-width: 50px; max-height: 50px;" alt="">
                                    </td>
                                    <td>
                                        <center><%= products[i].productName %></center>
                                    </td>
                                    <td>
                                        <center><%= products[i].quantity%></center>
                                    </td>
                                    <td>
                                        <center><%= products[i].price%></center>
                                    </td>
                                    <td>
                                        <center><%= products[i].category%></center>
                                    </td>
                                    <td>
                                       <center><%= products[i].offer%></center> 
                                    </td>
                                    <td> 
                                        <a class="badge rounded-pill badge-dark" href="/admin/editProduct/<%=products[i].productName%>">
                                            edit
                                        </a>
                                    </td>
                                    <td>
                                        <% if (products[i].isBlocked==true){%>
                                            <a class="badge rounded-pill badge-success" onclick="listAndUnlist('<%= products[i].productName%>',false)">
                                                list
                                            </a>
                                            <%}else{%>
                                                <a class="badge rounded-pill badge-danger" onclick="listAndUnlist('<%= products[i].productName%>',true)">
                                                    unlist
                                                </a>
                                                <%}%>
                                    </td>
                                </tr>
                                <%}%>
                        </tbody>
                    </table> <!-- table-responsive.// -->
                    <!-- firstModal --- Edit Category -->

                </div>
            </div>
            <!-- card-body end// -->

        </div>
        <!-- content-main end/ -->
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script>
                        document.write(new Date().getFullYear());
                    </script>

                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">All rights reserved</div>
                </div>
            </div>
        </footer>
    </main>
    <script src="/adminAssets/js/vendors/jquery-3.6.0.min.js"></script>
